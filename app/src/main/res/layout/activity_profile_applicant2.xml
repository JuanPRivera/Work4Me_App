<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="none"
    android:tag="profile_Applicant">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center_horizontal"
        android:layout_weight="1"
        android:background="@color/background"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        android:paddingStart="25dp"
        android:paddingEnd="25dp">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_marginTop="20dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="left"
                android:clickable="true"
                android:contentDescription="back"
                android:onClick="onClickBack"
                app:srcCompat="@drawable/arrow" />

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="5dp"
                android:contentDescription="@string/login_button"
                app:srcCompat="@drawable/imageuser" />
        </FrameLayout>

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="My profile"
            android:textColor="@color/white"
            android:textSize="28sp" />

        <ImageView
            android:id="@+id/activeBgPrApp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="20dp"
            android:src="@drawable/ic_tab_active"
            tools:srcCompat="@drawable/ic_applicant_btn_active" />

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_marginTop="15dp">

            <TextView
                android:id="@+id/textViewNamePrApp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="@string/name_input"
                android:textColor="#616161"
                android:textSize="22sp"
                tools:ignore="SpeakableTextPresentCheck" />

            <EditText
                android:id="@+id/editTextNamePrApp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:background="@android:color/transparent"
                android:cursorVisible="true"
                android:editable="false"
                android:importantForAutofill="no"
                android:inputType="none"
                android:textColor="@color/dark_font"
                android:textCursorDrawable="@drawable/input_cursor"
                android:textSize="22sp"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom">

                <View
                    android:id="@+id/viewBackNamePrApp"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:background="@color/dark_font" />

                <View
                    android:id="@+id/viewBlueNamePrApp"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_gravity="center_horizontal"
                    android:background="@color/blue"
                    android:scaleX="0" />
            </FrameLayout>
        </FrameLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_marginTop="15dp">

            <TextView
                android:id="@+id/textViewLastnamePrApp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="@string/lastname_input"
                android:textColor="#616161"
                android:textSize="22sp"
                tools:ignore="SpeakableTextPresentCheck" />

            <EditText
                android:id="@+id/editTextLastnamePrApp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:background="@android:color/transparent"
                android:cursorVisible="true"
                android:editable="false"
                android:importantForAutofill="no"
                android:inputType="none"
                android:textColor="@color/dark_font"
                android:textCursorDrawable="@drawable/input_cursor"
                android:textSize="22sp"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom">

                <View
                    android:id="@+id/viewBackLastNamePrApp"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:background="@color/dark_font" />

                <View
                    android:id="@+id/viewBlueLastNamePrApp"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_gravity="center_horizontal"
                    android:background="@color/blue"
                    android:scaleX="0" />
            </FrameLayout>

        </FrameLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_marginTop="15dp">

            <TextView
                android:id="@+id/textViewIdPrApp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="@string/id_input"
                android:textColor="#616161"
                android:textSize="22sp"
                tools:ignore="SpeakableTextPresentCheck" />

            <EditText
                android:id="@+id/editTextIdPrApp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:background="@android:color/transparent"
                android:cursorVisible="true"
                android:editable="false"
                android:importantForAutofill="no"
                android:inputType="none"
                android:textColor="@color/dark_font"
                android:textCursorDrawable="@drawable/input_cursor"
                android:textSize="22sp"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom">

                <View
                    android:id="@+id/viewBackIdPrApp"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:background="@color/dark_font" />

                <View
                    android:id="@+id/viewBlueIdPrApp"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_gravity="center_horizontal"
                    android:background="@color/blue"
                    android:scaleX="0" />
            </FrameLayout>

        </FrameLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_marginTop="15dp">

            <TextView
                android:id="@+id/textViewCityPrApp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="@string/city_input"
                android:textColor="#616161"
                android:textSize="22sp"
                tools:ignore="SpeakableTextPresentCheck" />

            <EditText
                android:id="@+id/editTextCityPrApp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:background="@android:color/transparent"
                android:cursorVisible="true"
                android:editable="false"
                android:importantForAutofill="no"
                android:inputType="none"
                android:textColor="@color/dark_font"
                android:textCursorDrawable="@drawable/input_cursor"
                android:textSize="22sp"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom">

                <View
                    android:id="@+id/viewBackCityPrApp"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:background="@color/dark_font" />

                <View
                    android:id="@+id/viewBlueCityPrApp"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_gravity="center_horizontal"
                    android:background="@color/blue"
                    android:scaleX="0" />
            </FrameLayout>

        </FrameLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_marginTop="15dp">

            <TextView
                android:id="@+id/textViewBirthdayPrApp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="@string/birthday_input"
                android:textColor="#616161"
                android:textSize="22sp"
                tools:ignore="SpeakableTextPresentCheck" />

            <EditText
                android:id="@+id/editTextBirthdayPrApp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:background="@android:color/transparent"
                android:cursorVisible="true"
                android:editable="false"
                android:importantForAutofill="no"
                android:inputType="none"
                android:textColor="@color/dark_font"
                android:textCursorDrawable="@drawable/input_cursor"
                android:textSize="22sp"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom">

                <View
                    android:id="@+id/viewBackBirthdayPrApp"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:background="@color/dark_font" />

                <View
                    android:id="@+id/viewBlueBirthdayPrApp"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_gravity="center_horizontal"
                    android:background="@color/blue"
                    android:scaleX="0" />
            </FrameLayout>

        </FrameLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_marginTop="15dp">

            <TextView
                android:id="@+id/textViewPhonePrApp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="@string/phone_input"
                android:textColor="#616161"
                android:textSize="22sp"
                tools:ignore="SpeakableTextPresentCheck" />

            <EditText
                android:id="@+id/editTextPhonePrApp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:background="@android:color/transparent"
                android:cursorVisible="true"
                android:editable="false"
                android:importantForAutofill="no"
                android:inputType="none"
                android:textColor="@color/dark_font"
                android:textCursorDrawable="@drawable/input_cursor"
                android:textSize="22sp"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom">

                <View
                    android:id="@+id/viewBackPhonePrApp"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:background="@color/dark_font" />

                <View
                    android:id="@+id/viewBluePhonePrApp"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_gravity="center_horizontal"
                    android:background="@color/blue"
                    android:scaleX="0" />
            </FrameLayout>

        </FrameLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_marginTop="15dp">

            <TextView
                android:id="@+id/textViewEmailPrApp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="@string/email_input"
                android:textColor="#616161"
                android:textSize="22sp"
                tools:ignore="SpeakableTextPresentCheck" />

            <EditText
                android:id="@+id/editTextEmailPrApp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:background="@android:color/transparent"
                android:clickable="true"
                android:cursorVisible="true"
                android:editable="false"
                android:importantForAutofill="no"
                android:inputType="none"
                android:textColor="@color/dark_font"
                android:textCursorDrawable="@drawable/input_cursor"
                android:textSize="22sp"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom">

                <View
                    android:id="@+id/viewBackEmailPrApp"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:background="@color/dark_font" />

                <View
                    android:id="@+id/viewBlueEmailPrApp"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_gravity="center_horizontal"
                    android:background="@color/blue"
                    android:scaleX="0" />
            </FrameLayout>

        </FrameLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_marginTop="15dp">

            <TextView
                android:id="@+id/textViewPasswordPrApp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="New Password"
                android:textColor="#616161"
                android:textSize="22sp"
                tools:ignore="SpeakableTextPresentCheck" />

            <EditText
                android:id="@+id/editTextPasswordPrApp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:background="@android:color/transparent"
                android:cursorVisible="true"
                android:importantForAutofill="no"
                android:inputType="textPassword"
                android:textColor="@color/dark_font"
                android:textCursorDrawable="@drawable/input_cursor"
                android:textSize="22sp"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom">

                <View
                    android:id="@+id/viewBackPassword"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:background="@color/dark_font" />

                <View
                    android:id="@+id/viewBluePassword"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_gravity="center_horizontal"
                    android:background="@color/blue"
                    android:scaleX="0" />
            </FrameLayout>

        </FrameLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_marginTop="15dp">

            <TextView
                android:id="@+id/textViewConfirmNewPassPrApp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="Confirmation New Password"
                android:textColor="#616161"
                android:textSize="22sp"
                tools:ignore="SpeakableTextPresentCheck" />

            <EditText
                android:id="@+id/editTextConfirmNewPassPrApp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:background="@android:color/transparent"
                android:cursorVisible="true"
                android:importantForAutofill="no"
                android:inputType="textPassword"
                android:textColor="@color/dark_font"
                android:textCursorDrawable="@drawable/input_cursor"
                android:textSize="22sp"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom">

                <View
                    android:id="@+id/viewBackConfirmNewPassPrApp"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:background="@color/dark_font" />

                <View
                    android:id="@+id/viewBlueConfirmNewPassPrApp"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_gravity="center_horizontal"
                    android:background="@color/blue"
                    android:scaleX="0" />
            </FrameLayout>
        </FrameLayout>

        <ImageView
            android:id="@+id/UpdateButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="50dp"
            android:layout_marginBottom="70dp"
            android:clickable="true"
            android:contentDescription="Update"
            android:onClick="onTapUpdate"
            app:srcCompat="@drawable/buttonupdate" />
    </LinearLayout>

</ScrollView>